@page "/qrview"
@using Microsoft.AspNetCore.Components.Web
@using Azox.Toolkit.Blazor
@namespace Azox.XQR.Presentation.Areas.QrView.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="tr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />
    <base href="/qrview" />
    <link rel="stylesheet" href="~/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/dist/css/font/bootstrap-icons.min.css" />
    <link rel="stylesheet" href="~/dist/css/qrview.min.css" asp-append-version="true" />
    <component type="typeof(HeadOutlet)" render-mode="ServerPrerendered" />
</head>
<body>
    <component type="typeof(QrViewApp)" render-mode="ServerPrerendered" />
    <component type="typeof(ToastContainer)" render-mode="ServerPrerendered" />
    <script src="_framework/blazor.server.js"></script>
    <script src="~/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>