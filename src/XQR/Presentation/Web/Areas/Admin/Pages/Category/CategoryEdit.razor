@page "/admin/category/{CategoryId:int}"

<AuthorizeView Roles="@($"{nameof(UserGroupType.Admin)}, {nameof(UserGroupType.MerchantAdmin)},{nameof(UserGroupType.ServiceAdmin)}")">
    <Authorized>
        <CascadingValue Value="Model">
            <CategoryDetail />
        </CascadingValue>
    </Authorized>
    <NotAuthorized>
        <NotAuthorizedPageView />
    </NotAuthorized>
</AuthorizeView>